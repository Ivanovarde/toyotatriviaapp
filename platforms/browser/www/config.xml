<?xml version='1.0' encoding='utf-8'?>
<widget id="com.toyotatrivia.app" varsionCode="1" version="29.08.20" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name short="Toyota Trivia">Toyota Trivia App</name>
    <description>
		Trivia para salón de exposición Toyota.
	</description>
    <author email="iv@neomedia.com.ar" href="http://neomedia.com.ar">
		Ivan Varde
	</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="phonegap-version" value="cli-6.5.0" />
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="true" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="InAppBrowserStorageEnabled" value="true" />
    <preference name="LoadUrlTimeoutValue" value="60000" />
</widget>
